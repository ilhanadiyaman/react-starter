---
  parser: babel-eslint
  extends:
    - airbnb
  env:
    browser: true
    node: true
    es6: true
    jest: true
  plugins:
    - react
    - jest
  rules:
    react/jsx-filename-extension: [1, {"extensions": [".js", ".jsx"]}]

    max-len: [2, 200, { code: 200, ignoreUrls: true, ignoreStrings: true, ignoreTemplateLiterals: true }]
